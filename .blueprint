created:
    - database/factories/StudentFactory.php
    - database/factories/ProgramFactory.php
    - database/factories/SchoolFactory.php
    - database/factories/ArchivedStudentFactory.php
    - database/migrations/2024_11_19_133445_create_students_table.php
    - database/migrations/2024_11_19_133446_create_programs_table.php
    - database/migrations/2024_11_19_133447_create_schools_table.php
