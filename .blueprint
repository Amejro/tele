    - app/Models/Program.php
    - app/Models/School.php
    - app/Models/ArchivedStudent.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
    Student: { name: string, email: 'string unique', student_id: 'string unique', program_id: 'id foreign:Program', telephone: string, level: integer, 'program_type:enum': 'regular, top_up', telcost_number: string, expected_completion_year: integer, relationships: { belongsTo: Program } }
    Program: { program_name: string, regular_duration: integer, top_up_duration: 'integer nullable', has_top_up: boolean, school_id: 'id foreign:School', timestamps: timestamps, relationships: { hasMany: Student, belongsTo: School } }
    School: { name: string, relationships: { hasMany: Program } }
    ArchivedStudent: { name: string, email: 'string unique', student_id: 'string unique', program_id: 'id foreign:Program', telephone: string, 'program_type:enum': 'regular, top_up', telcost_number: string, expected_completion_year: integer, relationships: { belongsTo: Program } }
